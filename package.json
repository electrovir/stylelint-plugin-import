    },
    "dependencies": {
        "stylelint-rule-creator": "5.1.0"
    },
    "devDependencies": {
        "@types/jest": "28.1.8",
        "@types/stylelint": "13.13.3",
        "jest": "29.0.1",
        "postcss": "8.4.16",
        "postcss-less": "6.0.0",
        "stylelint": "14.11.0",
        "stylelint-jest-rule-tester": "2.1.7",
        "virmator": "1.4.6"
    },
    "optionalDependencies": {
        "fsevents": "2.3.2"
    },
    "peerDependencies": {
        "stylelint": "<16.0.0"
