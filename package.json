{
    "name": "stylelint-plugin-import",
    "version": "1.0.13",
    "description": "Stylelint plugin for managing imports.",
    "keywords": [
        "css",
        "less",
        "scss",
        "sass",
        "import",
        "stylelint",
        "file",
        "name",
        "file name",
        "reference",
        "extension",
        "file extension"
    ],
    "homepage": "https://github.com/electrovir/stylelint-plugin-import#readme",
    "bugs": {
        "url": "https://github.com/electrovir/stylelint-plugin-import/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/electrovir/stylelint-plugin-import.git"
    },
    "license": "MIT",
    "author": {
        "name": "electrovir",
        "url": "https://github.com/electrovir"
    },
    "main": "dist/index.js",
    "scripts": {
        "format": "virmator format write",
        "prepublishOnly": "npm run test:full",
